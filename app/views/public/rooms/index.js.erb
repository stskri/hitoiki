$("#rooms").append('<%= j render 'public/rooms/rooms', rooms: @rooms %>');

<% if @rooms.next_page %>
  $("#more_rooms_btn").html('<%= j link_to_next_page @rooms, "さらに読み込む", class: "btn btn-info btn-sm", remote: true %>');
<% else %>
  $("#more_rooms_btn").remove();
  $("#rooms").append('<div class="my-4 d-flex justify-content-center text-muted" style="font-size: 12px;">最下部です</div>')
<% end %>