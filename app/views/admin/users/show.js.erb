$("#admin_user_posts").append('<%= j render "admin/posts/posts_ajax", posts: @user_posts %>');

<% if @user_posts.next_page %>
  $("#admin_user_posts_more_btn").html('<%= j link_to_next_page @user_posts, "さらに読み込む", class: "btn btn-info btn-sm", remote: true %>');
<% else %>
  $("#admin_user_posts_more_btn").remove();
  $("#admin_user_posts").append('<div class="my-4 d-flex justify-content-center text-muted" style="font-size: 12px;">最下部です</div>')
<% end %>